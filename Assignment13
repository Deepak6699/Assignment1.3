package acadgild1;
import java.util.Scanner;
public class Assign13 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number into variable a");
		int a=sc.nextInt();
		System.out.println("Enter number into variable b");
		int b=sc.nextInt();
		  a = a ^ b;  
		  b = a ^ b; 
		  a = a ^ b;  
		  System.out.println("after swapping numbers be like a="+a+" b="+b);
		  sc.close();

	}

}
